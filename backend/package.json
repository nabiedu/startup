{
  "name": "dokwork-backend",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.0",
    "express-validator": "^6.14.3",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "prom-client": "^14.0.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "k6": "^0.0.0",
    "nodemon": "^2.0.22",
    "prisma": "^5.8.0",
    "supertest": "^6.3.3"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate:sqlite": "prisma generate --schema=prisma/schema.sqlite.prisma",
    "prisma:migrate:sqlite": "prisma migrate dev --schema=prisma/schema.sqlite.prisma --name init_sqlite",
    "seed": "node prisma/seed.js",
    "start:sqlite": "node index.js",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:integration": "node scripts/runIntegrationTests.js",
    "test:endpoints": "node scripts/test-endpoints.js",
    "load:test": "echo 'Run: k6 run scripts/load-test.k6.js' && echo 'Note: Install k6 first: https://k6.io/docs/getting-started/installation/'",
    "cleanup:testdb": "node -e \"try{require('fs').unlinkSync('./test.db');console.log('removed test.db')}catch(e){}\"",
    "test:watch": "NODE_ENV=test jest --watch"
  }
}
